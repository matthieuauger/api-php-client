{
    "name": "ma-residence/mr-api-php-client",
    "type": "library",
    "description": "Client library for ma-residence API",
    "keywords": ["ma-residence, maresidence"],
    "homepage": "http://www.ma-residence.fr",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/cache-subscriber": "dev-master@dev",
        "symfony/http-foundation": "~2.3"
    },
    "require-dev": {
      "phpunit/phpunit": "3.7.*",
      "phake/phake": "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "MaResidence\\Component\\ApiClient\\":"src/MaResidence/Component/ApiClient"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
